{
  "manifest_version": 3,
  "name": "Open extension API reference",
  "version": "1.0.0",
  "permissions": ["storage"],
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "service_worker": "service-worker.js",
	"type": "module"
  },
  "minimum_chrome_version": "102",
  "omnibox": {
	"keyword": "api"
  }
}